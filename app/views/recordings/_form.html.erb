<div class="row">
	<div class="col-md-4 col-md-offset-4">
		<%= bootstrap_form_for([@location, @location.recordings.build]) do |f| %>
			<div class="field">
				<%= f.number_field :temp, class: "form-control" %>
			</div>

			<div class="field">
				<%= f.text_field :status, class: "form-control" %>
			</div>

			<%= f.submit "Save Location", class: "form-control" %>
		<% end %>
	</div>
</div>